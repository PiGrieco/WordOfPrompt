# Configurazione di test per WordOfPrompt
# Copia questo file in config/.env e inserisci le tue API keys

# Hugging Face API (richiesta per OpenSesame)
HUGGINGFACE_API_KEY=hf_your_actual_key_here
HUGGINGFACE_MODEL_ID=PiGrieco/OpenSesame

# OpenAI API (opzionale)
OPENAI_API_KEY=sk-your_openai_key_here
OPENAI_MODEL=gpt-4-turbo

# Rainforest API (per ricerca Amazon)
RAINFOREST_API_KEY=your_rainforest_key_here

# Amazon Affiliate
AMAZON_AFFILIATE_ID=your_affiliate_tag_here

# Configurazione intent
INTENT_THRESHOLD=0.95
KEYWORD_METHOD=rake
MAX_KEYWORDS=5
